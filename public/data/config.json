{
  "src": "data/live.json",
  "orders": {
    "facorder": [
      "Cloakbot Factory",
      "Shieldbot Factory",
      "Spider Factory",
      "Jumpbot Factory",
      "Amphbot Factory",
      "Hovercraft Platform",
      "Rover Assembly",
      "Tank Foundry",
      "Gunship Plant",
      "Airplane Plant",
      "Shipyard",
      "Strider Hub",
      "Never",
      "Any"
    ]
  },
  "colors": {
    "fac": {
      "Cloakbot Factory": "#498fcb",
      "Shieldbot Factory": "#cb6e49",
      "Rover Assembly": "#d7a43e",
      "Hovercraft Platform": "#66af9a",
      "Spider Factory": "#68ad52",
      "Jumpbot Factory": "#ad527a",
      "Tank Foundry": "#413582",
      "Amphbot Factory": "#40cdcc",
      "Airplane Plant": "#eff632",
      "Gunship Plant": "#b2ee65",
      "Shipyard": "#6e8df0",
      "Strider Hub": "#ee0e08",
      "Never": "#ffffff"
    }
  },
  "columns": [
    {
      "type": "med",
      "charts": [
        {
          "vis": "bar",
          "dim": "winner_elo_lead",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Winner's ELO Advantage"
        },{
          "vis": "bar",
          "dim": "winner_elo",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Winner's ELO Rating"
        },{
          "vis": "bar",
          "dim": "loser_elo",
          "group": 100,
          "elasticX": true,
          "dim_pretty": "Loser's ELO Rating"
        },{
          "vis": "bar",
          "dim": "duration",
          "group": 5000,
          "elasticX": true,
          "dim_pretty": "Game Time (Frames)",
          "verticalXAxisTicks": true
        }
      ]
    },{
      "type": "med",
      "charts": [
        {
          "vis": "sun",
          "dim": "winner_fac_prog",
          "dim_pretty": "Winner's Factory Progression",
          "colors": "fac"
        },{
          "vis": "sun",
          "dim": "loser_fac_prog",
          "dim_pretty": "Loser's Factory Progression",
          "colors": "fac"
        }
      ]
    },{
      "type": "thin",
      "charts": [
        {
          "vis": "pie",
          "dim": "map",
          "dim_pretty": "Matchmaking Map Used"
        },{
          "vis": "pie",
          "dim": "map_type",
          "dim_pretty": "Matchmaking Map Type"
        },{
          "vis": "pie",
          "dim": "mirror_match",
          "dim_pretty": "Mirror Match"
        },{
          "vis": "pie",
          "dim": "zk_version",
          "dim_pretty": "Zero-K Version"
        },{
          "vis": "pie",
          "dim": "spring_version",
          "dim_pretty": "Spring RTS Version"
        }
      ]
    },{
      "type": "wide",
      "charts": [
        {
          "vis": "bar",
          "dim": "started",
          "dates": true,
          "elasticX": true,
          "group": "Day",
          "dim_pretty": "Match start time",
          "verticalXAxisTicks": true
        },{
          "vis": "matchups",
          "dim": {
            "id": "matchups",
            "type": "triangle",
            "rows": "winner_fac",
            "cols": "loser_fac"
          },
          "verticalXAxisTicks": true,
          "order": "facorder",
          "dim_pretty": "Matchups",
          "show_any": "true"
        }
      ]
    },{
      "type": "tiny",
      "charts": [
        {
          "vis": "count",
          "dim": "count",
          "dim_pretty": "Filter"
        },{
          "vis": "records",
          "dim": "gameid",
          "dim_pretty": "Battles",
          "count": 30,
          "columns": [
            {
              "label": "",
              "prop": "gameid",
              "type": "link",
              "ref": "<a href='https://zero-k.info/Battles/Detail/{}'>{}</a>"
            }
          ],
          "order": "descending"
        }
      ]
    }
  ]
}
